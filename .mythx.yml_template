output: mythx.json
format: json

# https://mythx.io/

analyze:
    mode: quick
    create-group: true
    group-name: My fancy analysis
    solc: 0.8.4
    remappings:
        - "@openzeppelin/=/my/path/node_modules/@openzeppelin/"
        - "@nomiclabs=/my/path/node_modules/@nomiclabs/"
    contracts:
        - Contract1
        - Contract2
        - Contract3
        - Contract4
        - Contract5
